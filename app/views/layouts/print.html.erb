		<script type="text/javascript" charset="utf-8">
		// $('a').click(function(e) {
		//     e.preventDefault();
		// });
		
		$(document).ready(function(){
			$('a.showdesc').click(function(e) {
				var itemname = $(this).data("adjustitem");
				$('#' + itemname).toggle();
			    e.preventDefault();
			});
		
			$('a.showalldesc').click(function(e) {
				$('.drink_descs').toggle();
			    e.preventDefault();
			});
		
			$('a.hideitem').click(function(e) {
				var itemname = $(this).data("adjustitem");
				$('#' + itemname).hide();
			    e.preventDefault();
			});
			
			$('a.specialitem').click(function(e) {
				var itemname = $(this).data("adjustitem");
				if( $('#' + itemname + ' i').hasClass("fa") ) {
					$('#' + itemname + ' i').removeClass("fa").css('opacity', '0')
					
				} else {
					$('#' + itemname + ' i').addClass("fa").css('opacity', '1')
				}
			    e.preventDefault();
			});
			
		});
		
		</script>
		
