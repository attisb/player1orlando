<style type="text/css" media="screen">

/* -------------------------------------------------
	Reset
------------------------------------------------- */
*, *:before, *:after {
	text-rendering: optimizeLegibility;
	margin: 0; padding: 0;
	-moz-box-sizing: border-box; -webkit-box-sizing: border-box; box-sizing: border-box;
}

	html, body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, abbr, address, cite, code, del, dfn, em, img, ins, kbd, q, samp, small, strong, sub, sup, var, b, i, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, article, aside, canvas, details, figcaption, figure, footer, header, hgroup, menu, nav, section, summary, time, mark, audio, video { margin: 0; padding: 0; border: 0; outline: 0; font-size: 100%; vertical-align: baseline; background: transparent; }
	[hidden] { display: none; }

	img {border: 0;}
	html {font-size: 100.01%}
	html, body {min-height: 100%;}
	body, ul, ol, dl {margin: 0;}
	textarea {resize: vertical;}
	article, aside, audio, footer, header, nav, section, video {display: block;}
	html { font-size: 100%;}
	html, button, input, select, textarea { font-family: sans-serif; color: #000; }
	body { margin: 0; font-size: 1em; line-height: 1.4; }
	
	/*@import url(http://fonts.googleapis.com/css?family=Montserrat:400,700);*/
	@import url(http://fonts.googleapis.com/css?family=Muli|Montserrat);

	body, input, button {color: #000; font-family: 'Muli', Helvetica, Arial, sans-serif;}
	body {font-size: 14px; line-height: 20px;}

	p + p {margin-top: 10px;}
	p.lead {font-size: 18px; line-height: 24px;}

	strong, b {font-family: 'Montserrat', 'Helvetica Neue', Helvetica, Arial, sans-serif; font-weight: 400;}

	.tiny {font-size: 10px;}
	small, .small {font-size: 12px;}
	.large {font-size: 18px;}
	.float-left {float: left; margin-right: 20px;}
	.float-right {float: right; margin-left: 20px;}
	.text-left {text-align: left;}
	.text-center {text-align: center;}
	.text-right {text-align: right;}

	h1, h2, h3, h4, h5, h6 {font: 16px/1.5 'Montserrat', 'Helvetica Neue', Helvetica, Arial, sans-serif; font-weight: 400; margin: 0; margin-bottom: 5px; text-transform: uppercase;}
	h1 {font-size: 60px; line-height: 60px;}
	h2 {font-size: 48px; line-height: 48px;}
	h3 {font-size: 36px; line-height: 36px;}
	h4 {font-size: 24px; line-height: 24px;}
	h5 {font-size: 18px; line-height: 18px;}
	h6 {font-size: 12px; line-height: 12px;}
	h1 small, h2 small, h3 small, h4 small, h5 small, h6 small {opacity: .5; line-height: 12px; font-size: 12px; display: block;}
	h3 i {font-size: 80%;}

	
	body {background: #f6f8f8; color: #000;}
	body.dark {background: #000; color: #ddd;}
	.dark a {color: #60a8c0;}
	.dark .hero {color: #fff;}
	.dark header {background: #080d12; border-bottom: 1px solid rgba(255,255,255,.5); box-shadow: 0px 3px 3px #000;}
	.dark header nav a {color: #fff;}
	.dark header nav a:hover {color: #fff;}
	.dark .link-black {color: #fff;}
	.dark .full_btn {background: #CF532D; border-color: #dbdbdb}
	.dark blockquote {border-color: #dbdbdb; color: #A5B3AD;}
	.dark strong {color: #fff;}
	.dark h1, .dark h2, .dark h3, .dark h4, .dark h5, .dark h6 {color: #fff;}
	.dark h2 {color: #fff;}
	.dark footer {margin-top: 100px; padding-top: 20px;}
	.dark footer {background: #080d12; border-top: 1px solid rgba(255,255,255,.5); box-shadow: 0px -3px 3px #000;}
	
	

	body {background: url(/includes/horbg.png) no-repeat !important; background-size: 100% 100%;}
#screen_liner {margin: auto; position: absolute; top: -40px; left: 40px; bottom: 0; right: 0;
	width: 1200px; height: 640px;
}
.slide {display: none; width: 1200px; height: 640px;}
.screen_header h1 {font-size: 50px; position: absolute; top: 0; left: 0; width: 1200px; height: 50px; line-height: 50px; padding-top: 10px; padding-left: 10px; text-shadow: 2px 2px 0px #000;}
.screen_column {position: absolute; top: 90px; width: 600px;}
.column2 {left: 600px;}
.screen_column ul {list-style: none; font-size: 16px; line-height: 16px; font-family: 'Montserrat', 'Helvetica Neue', Helvetica, Arial, sans-serif; font-weight: 400;}
.screen_column li {float: left; margin: 0; padding: 0; height: 60px; text-shadow: 1px 1px 0px #000;}
.screen_column li.screen_price {width: 80px; left: 0px; text-indent: 10px;}
.screen_column li.screen_abv {width: 80px; left: 80px;}
.screen_column li.screen_name {width: 440px; left: 160px;}
.screen_column li.screen_name h4 {color: #F39C12 !important; width: 420px; white-space: nowrap; overflow: hidden;}

	
</style>

</head>
<body class="dark" id="screen_view">	

<%= yield %>


</body>
